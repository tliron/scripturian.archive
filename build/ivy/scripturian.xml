<?xml version="1.0" encoding="UTF-8"?>
<ivy-module version="2.0">
	<info
		organisation="${maven.group}"
		module="${maven.artifact}"
		revision="${version}"
		status="release"
		default="true" />
	<configurations>
		<conf name="default" visibility="public" />
	</configurations>
	<dependencies>
		<dependency org="org.clojure" name="clojure" rev="1.3.0" transitive="false" />
		<dependency org="org.apache.commons" name="commons-daemon" rev="1.0.7" transitive="false" />
		<dependency org="org.codehaus.groovy" name="groovy" rev="2.0.1" transitive="false" />
		<dependency org="net.sourceforge.jepp" name="jepp" rev="2.4" transitive="false" />
		<dependency org="org.jruby" name="jruby-standalone" rev="1.6.7.2" transitive="false" />
		<dependency org="org.scala-lang" name="scala-compiler" rev="2.9.1" transitive="false" />
		<dependency org="org.scala-lang" name="scala-library" rev="2.9.1" transitive="false" />
		<dependency org="javax.script" name="script-api" rev="1" transitive="false" />
		<dependency org="com.threecrickets.jygments" name="jygments" rev="0.9.0" transitive="false" />
		<dependency org="org.python" name="jython-standalone" rev="2.5.3" transitive="false" />
		<dependency org="com.caucho" name="quercus" rev="4.0.25" transitive="true" />
		<dependency org="org.mozilla" name="rhino" rev="1.7R4" transitive="false" />
		<dependency org="com.threecrickets.succinct" name="succinct" rev="1.0.1" transitive="false" /> 
		<dependency org="org.apache.velocity" name="velocity" rev="1.7" transitive="false" />
		<dependency org="org.eclipse.mylyn" name="wikitext" rev="1.5.0" transitive="false" />
		<dependency org="org.pegdown" name="pegdown" rev="1.1.0" transitive="false" />
	</dependencies>
</ivy-module>
